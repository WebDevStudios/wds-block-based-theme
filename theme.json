{
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#a156b4",
					"name": "Strong magenta",
					"slug": "strong-magenta"
				},
				{
					"color": "#f1f1f1",
					"name": "Very light gray",
					"slug": "very-light-gray"
				}
			]
		},
		"custom": {
			"lineHeight": {
				"large": "2.5",
				"medium": "2",
				"small": "1.3"
			}
		},
		"typography": {
			"fontFamilies": [],
			"fontSizes": [
				{
					"size": "0.5rem",
					"slug": "small"
				},
				{
					"size": "1rem",
					"slug": "regular"
				},
				{
					"size": "2rem",
					"slug": "large"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/group": {
				"color": {
					"background": "#ffffff"
				}
			},
			"core/heading": {
				"elements": {
					"h2": {
						"color": {
							"text": "var( --wp--preset--color--strong-magenta )"
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--large)",
							"lineHeight": "var(--wp--preset--line-height--medium)"
						}
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"fontSize": "calc(10px * var(--wp--preset--font-size--regular)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--very-light-gray)"
		}
	},
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json"
}
